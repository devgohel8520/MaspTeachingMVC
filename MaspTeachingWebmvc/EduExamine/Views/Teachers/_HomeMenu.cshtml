@using EduExamine.Models;
@model IEnumerable<EduExamine.Models.Teacher>

@functions{
    public string classResult(TeacherType adminType, TeacherType menuId)
    {
        return Model.First().Types.Equals(adminType) && Model.First().Types.Equals(menuId) ? "col-md-12 onHoverActive" : "col-md-12 onHover";
    }
}

